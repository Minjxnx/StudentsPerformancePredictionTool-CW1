<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="StudySessionSoap" />
                <binding name="BreakSessionSoap" />
                <binding name="ReportSoap" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:59492/StudySession.asmx"
                binding="basicHttpBinding" bindingConfiguration="StudySessionSoap"
                contract="StudySessionService.StudySessionSoap" name="StudySessionSoap" />
            <endpoint address="http://localhost:59492/BreakSession.asmx"
                binding="basicHttpBinding" bindingConfiguration="BreakSessionSoap"
                contract="BreakSessionService.BreakSessionSoap" name="BreakSessionSoap" />
            <endpoint address="http://localhost:59492/Report.asmx" binding="basicHttpBinding"
                bindingConfiguration="ReportSoap" contract="ReportService.ReportSoap"
                name="ReportSoap" />
        </client>
    </system.serviceModel>
</configuration>